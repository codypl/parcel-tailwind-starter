{
  "name": "parcel-tailwind-starter",
  "version": "2.0.0",
  "description": "Simple TailwindCSS v2 starter with Parcel v2",
  "scripts": {
    "prebuild": "rimraf .cache && rimraf dist",
    "build": "npm run prebuild && parcel build src/*.html",
    "dev": "parcel src/*.html"
  },
  "license": "MIT",
  "private": true,
  "dependencies": {
    "autoprefixer": "9.8.6",
    "postcss": "7.0.35",
    "tailwindcss": "2.0.1-compat"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.0.0-beta.2",
    "parcel": "^2.0.0-beta.2",
    "rimraf": "^3.0.2"
  }
}
