{
  "name": "parcel-tailwind-starter",
  "version": "2.0.0",
  "description": "Simple TailwindCSS v3 starter with Parcel v2",
  "scripts": {
    "prebuild": "rimraf .cache && rimraf dist",
    "build": "npm run prebuild && parcel build src/*.html",
    "dev": "parcel src/*.html"
  },
  "license": "MIT",
  "private": true,
  "dependencies": {
    "parcel": "^2.12.0",
    "tailwindcss": "^3.4.1"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.12.0",
    "autoprefixer": "^10.4.17",
    "postcss": "^8.4.35"
  }
}
